{
  "name": "trancheready",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": { "node": ">=18 <=22" },
  "scripts": {
    "start": "node server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "lint": "node --run eslint",
    "eslint": "eslint .",
    "format": "prettier -w .",
    "check:audit": "npm audit --audit-level=moderate || true",
